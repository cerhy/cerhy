<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.jeecms.cms.entity.assist">
	<class name="CmsReportDoc" table="jc_report_doc">
		<meta attribute="sync-DAO">false</meta>
		<cache usage="read-write"/>
		<id name="id" type="integer" column="join_id"><generator class="identity"/></id>
		<property name="reportname" column="reportname" type="string" not-null="false" length="50"/>
		<property name="reporturl" column="reporturl" type="string" not-null="false" length="50"/>
		<property name="reporttime" column="reporttime" type="timestamp" not-null="false" length="50"/>
	</class>
</hibernate-mapping>